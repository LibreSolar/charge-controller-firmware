{
    "configurations": [
        {
            "name": "Zephyr",
            "browse": {
                "path": [
                    "${workspaceFolder}/src"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "compileCommands": "${workspaceFolder}/zephyr/build/compile_commands.json",
            "cStandard": "c99",
            "cppStandard": "c++14",
            "forcedInclude": [
                "${workspaceFolder}/zephyr/build/zephyr/include/generated/autoconf.h"
            ]
        }
    ],
    "version": 4
}
